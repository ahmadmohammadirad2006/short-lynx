{
  "name": "short-lynx",
  "version": "1.0.0",
  "description": "URL shortener",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon",
    "start": "set NODE_ENV=production&& nodemon",
    "watch:tailwindcss": "npx tailwindcss -i ./src/css/input.css -o ./src/css/output.css --watch",
    "watch:js": "parcel watch ./public/js/script.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/script.js --out-dir ./public/js --out-file bundle.js"
  },
  "author": "Ahmad Mohammadirad",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "mongoose": "^8.5.1",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.4",
    "tailwindcss": "^3.4.6"
  }
}
